# connect to a different hub server by uncommenting and modifying the hub field
# default hub is https://hub.k0s.io
#
# hub: https://hub.k0s.io

# log verbosely
#
# verbose: true

# skip verify hub https certificate
#
# insecure: false

# cache login credential so you don't need to type
# your password every time you connect to an agent from the cli
#
# make sure you have write permission to this file
cache_credentials: true
