go build -o client -ldflags="-X 'main.commit=$(git rev-list -1 HEAD)' -X 'main.built=$(date)' -X 'main.branch=$(git rev-parse --abbrev-ref HEAD)'" . 
sudo docker build -t invctrl/client-base -f Dockerfile.base .
sudo docker build -t invctrl/client:latest -f Dockerfile.local --no-cache .
docker push invctrl/client-base:latest 
docker push invctrl/client:latest 
